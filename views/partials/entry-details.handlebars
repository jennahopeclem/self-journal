<div class="row mb-4 project card">
  <div class="col-md-5">
    <h2>
      <p>{{name}}</p>
    </h2>
    <p>Entry by:
      {{#each entry.user as |user|}}
        {{! entry user's fname and lname is console logged properly, but how to display? }}
        {{user.first_name}}
        {{user.last_name}}
      {{/each}},
      {{format_date date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{text}}
    </p>
  </div>
</div>