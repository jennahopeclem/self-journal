{{#each entry as |entry|}}
<div class="row mb-4 project">
    <div class="col-md-5">
        <h2>
            <p>{{entry.name}}</p>
        </h2>
        <p>Created by: {{entry.user.first_name}} {{entry.user.last_name}} on {{format_date entry.date_created}}</p>
    </div>
    <div class="col-md-7">
        <p>
            {{entry.description}}
        </p>
    </div>
</div>
{{/each}}